{
  "compilerOptions": {
    "target": "ES2019",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "declaration": true,
    "moduleResolution": "Node",
    "stripInternal": true
  },
  "include": ["src/**/*"],
}
